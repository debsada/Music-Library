<html>
  <head></head>
  <body>
    <h1>Albums</h1>
        <div>
        <% @albums.each do |album| %>
            <p>
            Title: <%= album.title %>
            Released: <%= album.release_year %>
            <a href="/albums/<%= album.id %>" ><%= album.title %></a><br />
            </p>
        <% end %>
        </div>
    </body>
</html>